{
  "name": "dataform",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emoji-mart/data": "^1.1.2",
    "@emoji-mart/react": "^1.1.1",
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.17",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/fontfaceobserver": "^2.1.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^14.18.27",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/rimraf": "^3.0.2",
    "@types/shelljs": "^0.8.11",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.18.0",
    "chalk": "4.1.2",
    "cross-env": "7.0.3",
    "deep-object-diff": "^1.1.9",
    "emoji-mart": "^5.5.2",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "fontfaceobserver": "2.3.0",
    "husky": "8.0.1",
    "i18next": "21.9.2",
    "i18next-browser-languagedetector": "6.1.5",
    "i18next-scanner": "4.0.0",
    "immer": "^10.0.1",
    "inquirer": "7.3.3",
    "inquirer-directory": "2.2.0",
    "jest-styled-components": "7.1.1",
    "lint-staged": "13.0.3",
    "node-plop": "0.26.3",
    "object-path": "^0.11.8",
    "plop": "2.7.6",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-app-polyfill": "3.0.0",
    "react-best-gradient-color-picker": "^2.1.13",
    "react-color": "^2.19.3",
    "react-dom": "18.2.0",
    "react-helmet-async": "1.3.0",
    "react-i18next": "11.18.6",
    "react-icons": "4.8.0",
    "react-is": "18.2.0",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "react-select": "^5.7.2",
    "react-table": "^7.8.0",
    "react-test-renderer": "18.2.0",
    "react-use": "^17.4.0",
    "rimraf": "3.0.2",
    "sanitize.css": "13.0.0",
    "serve": "14.0.1",
    "shelljs": "0.8.5",
    "stylelint": "14.12.0",
    "stylelint-config-recommended": "9.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "swiper": "^9.2.3",
    "ts-node": "10.9.1",
    "typescript": "4.6.4",
    "uuid": "^9.0.0",
    "web-vitals": "2.1.4",
    "zustand": "^4.3.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:prod": "yarn run build && serve -s build",
    "test:generators": "ts-node ./internals/testing/generators/test-generators.ts",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "yarn run eslint src",
    "lint:fix": "yarn run eslint --fix src",
    "lint:css": "stylelint src/**/*.css",
    "generate": "plop --plopfile internals/generators/plopfile.ts",
    "cleanAndSetup": "ts-node ./internals/scripts/clean.ts",
    "prettify": "prettier --write",
    "format": "prettier --write \"src/**/*.ts\"  \"src/**/*.tsx\" \"test/**/*.ts\"",
    "extract-messages": "i18next-scanner --config=internals/extractMessages/i18next-scanner.config.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=14.x"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/messages.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-table": "^7.7.14",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  }
}
